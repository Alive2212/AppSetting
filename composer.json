{
    "name": "Alive2212/AppSetting",
    "type": "library",
    "description": "This package for get any setting to apps just one time for any configuration and setup",
    "keywords": [
        "alive2212",
        "app-setting"
    ],
    "homepage": "https://github.com/Alive2212/AppSetting",
    "license": "MIT",
    "authors": [
        {
            "name": "B. Nodoust",
            "email": "alive2212@yahoo.com",
            "homepage": "https://babakn.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "alive2212\\app-setting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "alive2212\\app-setting\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
